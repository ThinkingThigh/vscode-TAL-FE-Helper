{
  "name": "tal-fe",
  "displayName": "TAL-FE-Helper",
  "description": "just a snippets helper",
  "repository": {
    "type": "git",
    "url": "https://github.com/dreambo8563/vscode-TAL-FE-Helper.git"
  },
  "version": "0.0.4",
  "publisher": "dreambo8563",
  "engines": {
    "vscode": "^1.26.0"
  },
  "icon": "images/icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.initProject",
    "onCommand:extension.newPage",
    "onCommand:extension.extracTEXT"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.initProject",
        "title": "TAL-init"
      },
      {
        "command": "extension.newPage",
        "title": "TAL-page"
      },
      {
        "command": "extension.extracTEXT",
        "title": "ExtracTEXT"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "extension.extracTEXT",
          "group": "1_modification"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  }
}
